{
  "endpoint": "/v1/content/gallery",
  "method": "POST",
  "timeout": "5s",
  "output_encoding": "no-op",
  "input_headers": ["*"],
  "input_query_strings": ["title", "content", "tags"],
  "backend": [
    {
      "url_pattern": "/gallery",
      "encoding": "json",
      "sd": "static",
      "method": "POST",
      "host": ["{{ .contentVaultHost }}"]
    }
  ],
  "extra_config": {{ template "endpoint_extra_config.tmpl" . }}
}